<Window x:Class="Frigot.BajaProducto"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Frigot"
        mc:Ignorable="d"
        Title="BajaProducto" Height="234.167" Width="322.5" Background="#FFB42929">
    <Grid Margin="0,0,2,1">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="19*"/>
            <ColumnDefinition Width="377*"/>
        </Grid.ColumnDefinitions>
        <Label x:Name="nomProdu" Content="Nombre Producto:" HorizontalAlignment="Left" Margin="18,46,0,0" VerticalAlignment="Top" Foreground="#FFFDFCFC" Grid.Column="1"/>
        <TextBox x:Name="nombreProductoTxt" Grid.Column="1" HorizontalAlignment="Left" Height="24" Margin="144,48,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="EliminarBt" Content=" Eliminar" Grid.Column="1" HorizontalAlignment="Left" Margin="100,115,0,0" VerticalAlignment="Top" Width="75" Click="EliminarBt_Click"/>
        <Button x:Name="regresarBt" Content="Regresar" Grid.Column="1" HorizontalAlignment="Left" Margin="100,155,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>

    </Grid>
</Window>
